{
  "name": "astrbot-dashboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": "CodedThemes",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
