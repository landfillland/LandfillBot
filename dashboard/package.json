    "@mdi/font": "7.2.96",
    "@types/node": "^20.5.7",
    "@vitejs/plugin-vue": "6.0.3",
    "@vue/tsconfig": "0.4.0",
    "eslint": "^9.10.0",
    "sass": "1.66.1",
