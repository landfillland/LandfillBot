    "event-source-polyfill": "^1.0.31",
    "github-markdown-css": "^5.8.1",
    "highlight.js": "^11.11.1",
    "js-md5": "^0.8.3",
    "katex": "^0.16.27",
    "lodash": "4.17.21",
    "markstream-vue": "0.0.3-beta.7",
    "mermaid": "^11.12.2",
    "pinia": "2.1.6",
    "pinyin-pro": "^3.26.0",
    "shiki": "^3.20.0",
    "stream-markdown": "^0.0.11",
